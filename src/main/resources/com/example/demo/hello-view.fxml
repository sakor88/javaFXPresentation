<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
<VBox alignment="CENTER" prefHeight="800.0" prefWidth="1200.0" spacing="30.0">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label prefHeight="50.0" prefWidth="752.0" text="Zaawansowane Technologie Internetowe - JavaFX" textAlignment="CENTER">
    <font>
      <Font size="34.0" />
    </font>
  </Label>
  <Button maxHeight="50.0" maxWidth="300.0" mnemonicParsing="false" onAction="#onUIButtonClick" prefHeight="40.0" text="Elementy interfejsu użytkownika" id="UIButton" />
  <Button maxHeight="50.0" maxWidth="300.0" mnemonicParsing="false" onAction="#onTableButtonClick" prefHeight="40.0" text="Zarządzanie danymi" />
  <Button maxHeight="50.0" maxWidth="300.0" mnemonicParsing="false" onAction="#onGimmicksButtonClick" prefHeight="40.0" text="Wizualne ciekawostki" textAlignment="CENTER" />

</VBox>
</AnchorPane>
